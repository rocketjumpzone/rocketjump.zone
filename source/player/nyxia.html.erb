---
layout: player
name: &name "nyxia"
desc: "xonotic, i do some na tournaments"
title: *name
discord_id: "nyxia#3562"
country_tag: us
country_name: USA
clan: "-"
xon_name: "nyxia"
qc_name: "nyxiadash"
dbt_name: "nyxia"
mouse: "glorious D"
dpi: "500"
sens: "3"
accel: "0"
---

<div class="w3-auto w3-container">
  <div class="w3-row">
    <div class="w3-col l2 m2">
      <h1><%= current_page.data.name %></h1>
      <%= image_tag 'avatar/nyxia.png', class:'w3-mobile w3-circle player-avatar', alt: 'Nyxia' %>
      <div class="player-social">
        <a href="https://twitter.com/nyxiadash" target="_blank">
          <i class="fab fa-lg fa-twitter"></i>
        </a>
        <a href="https://twitch.tv/nyxiadash" target="_blank">
          <i class="fab fa-lg fa-twitch"></i>
        </a>
        <a href="https://www.youtube.com/channel/UCATTPZuJb8Tq6g64HBWa9Bw" target="_blank">
          <i class="fab fa-lg fa-youtube"></i>
        </a>
        <a href="https://steamcommunity.com/id/nyxiadash" target="_blank">
          <i class="fab fa-lg fa-steam"></i>
        </a>
        <div class="w3-tooltip mobile-tooltip w3-hide-small" style="display:inline;margin-left:2px">
          <i class="fab fa-lg fa-discord"></i>
          <span style="position:absolute;left:-50px;bottom:0;width:200px" class="w3-text w3-tag w3-padding w3-animate-opacity w3-round"><%= current_page.data.discord_id %></span>
        </div>
      </div>
    </div>

    <div class="w3-col l6 m6" style="margin-top:60px">
      <p><%= current_page.data.desc %></p>
      <div class="w3-panel" style="background:#131316">
        <p>
          <b><%= current_page.data.mouse %></b>
          &nbsp;&nbsp;
          <b>DPI:</b>
          <%= current_page.data.dpi %>&nbsp;&nbsp;
          <b>SENS:</b>
          <%= current_page.data.sens %>
          &nbsp;&nbsp;
          <b>ACCEL:</b>
          <%= current_page.data.accel %></p>
      </div>
    </div>

    <div class="w3-col l4 m4">
      <table class="w3-table">
        <tr>
          <td style="border-top:0 !important">
            <b>
              tourneys:</b>
          </td>
          <td style="border-top:0 !important">
            <b><%= data.players.nyxia.tourneys %></b>
            succeed
          </td>
        </tr>

        <tr>
          <td>
            <b>
              rank:</b>
          </td>
          <td>
            <i class="fas fa-bolt"></i> <b><%= data.players.nyxia.rank %></b>
          </td>
        </tr>

        <tr>
          <td>
            <b>
              country:</b>
          </td>
          <td><img src="../../images/flags/1x1/<%= current_page.data.country_tag %>.svg" width="20" height="20" alt="<%= current_page.data.country_tag %>">&nbsp;
            <%= current_page.data.country_name %></td>
        </tr>

        <tr>
          <td>
            <b>
              profiles:</b>
          </td>
          <td>
            <a style="border:none" href="http://stats.xonotic.org/player/64493" target="_blank"><%= image_tag '/images/xon-icon.png', alt: 'nyxia profile xonotic', title: 'Xonotic' %></a>
            <a style="border:none" href="https://stats.quake.com/profile/nyxiadash" target="_blank"><%= image_tag '/images/qc-icon.png', alt: 'nyxiadash profile quake champions', title: 'Quake Champions' %></a>
          </td>
        </tr>

        <tr>
          <td>
            <b>
              clan:</b>
          </td>
          <td><%= current_page.data.clan %></td>
        </tr>

        <tr>
          <td>
            <b>
              money earned:</b>
          </td>
          <td><%= data.players.nyxia.money %></td>
        </tr>

        <tr>
          <td>
            <b>
              trophies:
            </b>
          </td>
          <td>
            <i class="fas fa-trophy trophy-gold"></i>
            <b>
              <sup><%= data.players.nyxia.trophies.gold %></sup>
            </b>
            <i class="fas fa-trophy trophy-silver"></i>
            <b>
              <sup><%= data.players.nyxia.trophies.silver %></sup>
            </b>
            <i class="fas fa-trophy trophy-bronze"></i>
            <b>
              <sup><%= data.players.nyxia.trophies.bronze %></sup>
            </b>
          </td>
        </tr>

      </table>

    </div>
    <!-- end of w3-col l4-->
  </div>
  <!-- end of w3-row-->
  <hr>

  <div class="w3-row">
    <a href="javascript:void(0)" onclick="openCity(event, 'London');">
      <div class="w3-third tablink w3-bottombar w3-center w3-margin-bottom w3-border-red w3-padding">
        <b>HIGHLIGHT</b>
      </div>
    </a>
    <a href="javascript:void(0)" onclick="openCity(event, 'Paris');">
      <div class="w3-third tablink w3-bottombar w3-center w3-margin-bottom w3-padding">
        <b>TOURNEYS</b>
      </div>
    </a>
    <a href="javascript:void(0)" onclick="openCity(event, 'Tokyo');">
      <div class="w3-third tablink w3-bottombar w3-center w3-margin-bottom w3-padding">
        <b>DETAILS</b>
      </div>
    </a>
  </div>

  <div id="London" class="city w3-animate-opacity" style="display:block">
    <p>empty</p>
    <div class="sharethis-inline-reaction-buttons" style="margin-top:50px"></div>
  </div>

  <div id="Paris" class="city w3-animate-opacity" style="display:none">

  </div>

  <div id="Tokyo" class="city w3-animate-opacity" style="display:none">
    <p>DISCORD ID -
      <b><%= current_page.data.discord_id %></b><br><br>
      QUAKE NAME -
      <b><%= current_page.data.qc_name %></b><br>
      DIABOTICAL NAME -
      <b><%= current_page.data.dbt_name %></b><br>
      XONOTIC NAME -
      <b><%= current_page.data.xon_name %></b>
    </p>
    <hr>
  </div>

</div>
<!-- end of w3-auto-->
