              $('#clock0').countdown('2020/10/09 20:00:00').on('update.countdown', function (event) {
                var format = ' %H hr %M min ';
                if (event.offset.totalDays > 0) {
                  format = '%-d day%!d ' + format;
                }
                if (event.offset.weeks > 0) {
                  format = '%-w week%!w ' + format;
                }
                $(this).html(event.strftime(format));
              }).on('finish.countdown', function (event) {
                $(this).html('<a class="link-white" href="https://twitch.tv/rocketjumpzone" target="_blank">Watch Stream</a>').parent().addClass('disabled');
              });
            </script><br>
            <small>check in 7:30 - 7:45</small>
